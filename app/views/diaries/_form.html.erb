<%= render 'shared/error_messages' %>

<%= form_for diary do |f| %>
  <div class="form-group">
    <%= f.label :title, 'タイトル' %>
  　<%= f.text_field :title, class: 'form-control' %>
  </div>
  <div class="form-group">
    <%= f.label :body, '本文' %>
    <%= f.text_area :body, class:'form-control' %>
  </div>
  
    
  <%= f.submit '保存', class: 'btn btn-primary' %>
<% end %>